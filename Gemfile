source 'https://rubygems.org'

gem 'rails'

gem 'bootstrap-sass'

gem 'bcrypt-ruby'

gem 'faker'

gem 'will_paginate'
gem 'bootstrap-will_paginate'

group :development, :test do
  gem 'sqlite3'
  gem 'rspec-rails'
end

gem 'annotate', group: :development

# Gems used only for assets and not required
# in production environments by default.
